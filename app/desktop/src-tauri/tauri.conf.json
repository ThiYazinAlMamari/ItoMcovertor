{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "ItoMcovertor",
    "version": "1.0.0",
    "identifier": "com.itomcovertor.app",
    "build": {
        "frontendDist": "../src"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "title": "ItoMcovertor",
                "width": 800,
                "height": 600,
                "minWidth": 700,
                "minHeight": 500,
                "resizable": true,
                "center": true,
                "decorations": true
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "windows": {
            "certificateThumbprint": null,
            "digestAlgorithm": "sha256",
            "timestampUrl": ""
        }
    }
}